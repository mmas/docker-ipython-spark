{
    "display_name": "PySpark (Spark 1.5.2)",
    "language": "python",
    "argv": ["/usr/bin/python2","-m","IPython.kernel","-f","{connection_file}"],
    "env": {
        "SPARK_HOME": "/usr/lib/spark",
        "PYTHONPATH": "/usr/lib/spark/python/:/usr/lib/spark/python/lib/py4j-0.8.2.1-src.zip",
        "PYTHONSTARTUP": "/usr/lib/spark/python/pyspark/shell.py",
        "PYSPARK_SUBMIT_ARGS": "--master spark://HOSTNAME:7077 pyspark-shell"
    }
}